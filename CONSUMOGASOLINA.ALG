Algoritmo "consumoGasolina"

Var

   distanceTravelled, alcool, media: real

Inicio
      escreval("Distancia percorrida: ")
      leia(distanceTravelled)
      
      escreval("Combustivel gasto: ")
      leia(alcool)
      
      media <- distanceTravelled / alcool
      
      escreval("O consumo médio do carro é: ", media:8:3)


Fimalgoritmo